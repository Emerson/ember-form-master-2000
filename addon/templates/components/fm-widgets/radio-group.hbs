{{#each this.widgetAttrs.content as |option|}}
  {{fm-field
    widget='radio'
    display='radio'
    value=this.value
    targetValue=(get option this.widgetAttrs.optionValuePath)
    label=(get option this.widgetAttrs.optionLabelPath)
    onUserInteraction=(action 'radioButtonInteraction')
  }}
{{/each}}
